#import "../TestAPI.mapl"

int32 a = 9;
int32 b = 3;
int32 c = 5;

// PEMDAS
print("9*3+5", a*b+c);
print("9*3-5", a*b-c);
print("9/3+5", a/b+c);
print("9/3-5", a/b-c);
print("9*(3+5)", a*(b+c));
print("9*(3-5)", a*(b-c));
print("9/(3+5)", a/(b+c));
print("9/(3-5)", a/(b-c));

// Arithmetic takes precendence over bitwise.
print("9+3<<1", a+b<<1);
print("9-3<<1", a-b<<1);
print("9*3<<1", a*b<<1);
print("9/3<<1", a/b<<1);

print("9+3|5", a+b|c);
print("9-3|1", a-b|c);
print("9*3|1", a*b|c);
print("9/3|1", a/b|c);

// Modulo takes precedence over arithmetic.
print("5%3*9", c%b*a);

// Boolean logic takes precedence over negation.
bool b1 = false;
bool b2 = true;
print("!false || true", !b1 || b2);

