print(positiveAssignedFloat32, negativeAssignedFloat32, positiveDeclaredFloat32, negativeDeclaredFloat32);

// FLOAT64.
float64 positiveAssignedFloat64 = 6.54;
float64 negativeAssignedFloat64 = -6.54;

float64 positiveDeclaredFloat64;
positiveDeclaredFloat64 = 9.87;
float64 negativeDeclaredFloat64;
negativeDeclaredFloat64 = -9.87;

print(positiveAssignedFloat64, negativeAssignedFloat64, positiveDeclaredFloat64, negativeDeclaredFloat64);

// STRING.
string assignedString = "asdf";

string declaredString;
